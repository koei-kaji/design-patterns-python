{
  "env": {
    "browser": true,
    "es6": false,
    "es2021": false,
    "jasmine": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:jasmine/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "vars-on-top": "off"
  },
  "plugins": ["jasmine"]
}
